<%= render 'shared/navbar' %>
<h1> <%= @user.username %>'s wishlist!🎁 </h1>

<div class="container p-5">
    <div class="row">
        <div class="col-5 m-auto">
            <% @wishlist.reverse_each do |item| %>
                <%= render 'shared/card', game: item.game %>
                
            <% end %>

          </div>
        </div>
    </div>
</div>
