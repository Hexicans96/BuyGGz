<%= render 'shared/navbar' %>
<h1> Home page la</h1>
  <div class="container">
      <div class="row">
          <div class="col-5 m-auto">
            <% @games.reverse_each do |game| %>
              <%if game.user_id != current_user.id  %>
                <%= render 'shared/card', game: game %>
              <% end %>
            <% end %>
          </div>
      </div>
  </div>


<div><footer> <%= paginate @games %> </footer></div>

      
