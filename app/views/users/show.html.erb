<%= render 'shared/navbar' %>
<% if user_signed_in? %>
  <h1> <%= @user.username %> </h1>
<% end %>

<div class="container p-5">
    <div class="row">
        <div class="col-5 m-auto">
            <% @user.games.reverse_each do |game| %>
                <%= render 'shared/card', game: game %>

      
            <% end %>
          </div>
        </div>
    </div>
</div>